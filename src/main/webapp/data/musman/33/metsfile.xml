        <md:mods>
          <md:titleInfo xml:lang="dan">
            <md:title>Seks Sange af Ingemann, op. 45</md:title>
          </md:titleInfo>
          <md:originInfo>
            <md:dateIssued>1846</md:dateIssued>
          </md:originInfo>
          <md:note type="presentation" xml:lang="dan">
            <div xmlns="">Hartmann satte musik til Ingemanns digte allerede i 1845, og året efter sendte forfatteren ham sin samling <i>Blandede Digte</i>, som var udkommet i 1845. I oktober og november 1846 komponerede Hartmann seks af disse digte, deriblandt den meget populære børnesang <i>Stork, Stork, Langeben</i>. Flere af digtene er udsat strofisk, andre er gennemkomponerede, og i januar 1847 udkom samlingen på Lose &amp; Delbancos forlag.<br/><br/><a href="http://www.kb.dk/elib/noder/hartmann/worktexts/#33">English version</a></div>
          </md:note>
          <md:note type="caption" xml:lang="dan">
            <div xmlns="">[Seks Sange af Ingemann]<br/> Datering: 22.10.1846-26.11.1846<br/>12 + 3 s.</div>
          </md:note>
          <md:relatedItem type="host">
            <md:titleInfo xml:lang="dan">
              <md:title>Musikafdelingen</md:title>
            </md:titleInfo>
            <md:identifier type="uri" xml:lang="dan">http://www.kb.dk/musik</md:identifier>
          </md:relatedItem>
          <md:identifier type="signature">Hartmann Samling</md:identifier>
          <md:identifier type="thumb">http://img.kb.dk/ma/musman/nyhartmann/forsider/inge0000.jpg</md:identifier>
        </md:mods>
      </m:xmlData>
