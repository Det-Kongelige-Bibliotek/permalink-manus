        <md:mods>
          <md:titleInfo xml:lang="dan">
            <md:title>En Sommerdag</md:title>
          </md:titleInfo>
          <md:originInfo>
            <md:dateIssued>1854</md:dateIssued>
          </md:originInfo>
          <md:note type="presentation" xml:lang="dan">
            <div xmlns="">Med Henrik Hertz' gendigtning af folkevisen <i>Jeg gik mig ud en Sommerdag</i> som tekst skrev Hartmann i 1854 dette romantiske korværk, som hurtigt blev populært både i Musikforeningen i København og ude i landet.<br/><br/><a href="http://www.kb.dk/elib/noder/hartmann/worktexts/#7">English version</a></div>
          </md:note>
          <md:note type="caption" xml:lang="dan">
            <div xmlns=""><i>En Sommerdag. Idyl for Solo, Chor og Orchester</i><br/>Datering: 25.11.1854<br/> 1 bd. 36 s. 34,6 x 24,2</div>
          </md:note>
          <md:relatedItem type="host">
            <md:titleInfo xml:lang="dan">
              <md:title>Musikafdelingen</md:title>
            </md:titleInfo>
            <md:identifier type="uri" xml:lang="dan">http://www.kb.dk/musik</md:identifier>
          </md:relatedItem>
          <md:identifier type="signature">Hartmanns samling</md:identifier>
          <md:identifier type="thumb">http://img.kb.dk/ma/musman/nyhartmann/forsider/somd0006.jpg</md:identifier>
        </md:mods>
      </m:xmlData>
    </m:mdWrap>
